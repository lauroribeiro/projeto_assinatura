<%= form_with(model: [@request, @request.users.build ], local:
true) do |form| %>
  <p>
    <%= form.label 'Nome' %><br>
    <%= form.text_field :name %><br>
  </p>
  <p>
    <%= form.label 'E-mail' %><br>
    <%= form.text_field :email %><br>
  </p>
  <p>
    <%= form.submit 'Adicionar' %>
  </p>
<% end %>